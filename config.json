{
  "embedded_patterns": {
    "stdout": {
      "pattern": "%STDOUT"
    },
    "stderr": {
      "pattern": "%STDERR"
    },
    "command": {
      "pattern": "!\\{(.+?)\\}"
    },
    "file": {
      "pattern": "%\\{(.+?)\\}"
    }
  },
  "extraction pattern": {
    "code_block": {
      "pattern": "^```(.*?)\\n([\\s\\S]*?)^```"
    }
  },
  "terminal_colors": {
    "prompt": {
      "fore": "green",
      "back": "default"
    },
    "chatgpt": {
      "fore": "default",
      "back": "default"
    },
    "warning": {
      "fore": "yellow",
      "back": "default"
    },
    "debug": {
      "fore": "cyan",
      "back": "default"
    },
    "stdout": {
      "fore": "yellow",
      "back": "default"
    },
    "stderr": {
      "fore": "yellow",
      "back": "default"
    }
  }
}
