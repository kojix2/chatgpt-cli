{
  "substitute_patterns": {
    "stdout": {
      "pattern": "%STDOUT"
    },
    "stderr": {
      "pattern": "%STDERR"
    },
    "command": {
      "pattern": "!\\{(.+?)\\}"
    },
    "url": {
      "pattern": "%%\\{(.+?)\\}"
    },
    "file": {
      "pattern": "%\\{(.+?)\\}"
    }
  },
  "terminal_colors": {
    "chatgpt": {
      "fore": "green",
      "back": "default"
    },
    "warning": {
      "fore": "yellow",
      "back": "default"
    },
    "debug": {
      "fore": "cyan",
      "back": "default"
    },
    "stdout": {
      "fore": "yellow",
      "back": "default"
    },
    "stderr": {
      "fore": "yellow",
      "back": "default"
    }
  }
}
